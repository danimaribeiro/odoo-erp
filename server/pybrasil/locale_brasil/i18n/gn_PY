<U0027> <puso-guarani>;<CAP>;<BAS>;IGNORE  # '
<U02BC> <puso-guarani>;<CAP>;<BAS>;IGNORE  # ʼ
<U0241> <puso-guarani>;<CAP>;<BAS>;IGNORE  # Ɂ
<U0242> <puso-guarani>;<MIN>;<BAS>;IGNORE  # ɂ
<UA78B> <puso-guarani>;<CAP>;<BAS>;IGNORE  # Ꞌ

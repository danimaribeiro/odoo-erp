reorder-after <gn-guarani>
<GN> <gn-guarani>;<CAP>;<BAS>;IGNORE  # G̃
<gn> <gn-guarani>;<MIN>;<BAS>;IGNORE  # g̃

reorder-after <nn-guarani>
<U00D1> <nn-guarani>;<CAP>;<BAS>;IGNORE  # Ñ
<U00F1> <nn-guarani>;<MIN>;<BAS>;IGNORE  # ñ

reorder-after <ch-guarani>
<CH> <ch-guarani>;<CAP>;<BAS>;IGNORE
<Ch> <ch-guarani>;<CAP>;<BAS>;IGNORE
<ch> <ch-guarani>;<MIN>;<BAS>;IGNORE
<cH> <ch-guarani>;<MIN>;<BAS>;IGNORE

reorder-after <mb-guarani>
<MB> <mb-guarani>;<CAP>;<BAS>;IGNORE
<Mb> <mb-guarani>;<CAP>;<BAS>;IGNORE
<mb> <mb-guarani>;<MIN>;<BAS>;IGNORE

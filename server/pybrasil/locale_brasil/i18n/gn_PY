LC_TIME
###############################

#          "atĩ";"akõ";"apy";"ard";"apo";"apt";"apk"
abday      "at<U0129>";"ak<U00F5>";"apy";"ard";"apo";"apt";"apk"

#          "arateĩ";"arakõi";"araapy";"ararundy";"arapo";"arapoteĩ";"arapokõi"
day        "arate<U0129>";"arak<U00F5>i";"ara<U02BC>apy";"ararundy";"arapo";"arapote<U0129>";"arapok<U00F5>i"

#          "jtĩ";"jkõ";"jpy";"jrd";"jpo";"jpt";"jpk";"jpp";"jpr";"jpa";"jaĩ";"jak"
abmon      "jt<U0129>";"jk<U00F5>";"jpy";"jrd";"jpo";"jpt";"jpk";"jpp";"jpr";"jpa";"ja<U0129>";"jak"

#          "jasyteĩ";"jasykõi";"jasyapy";"jasyrundy";"jasypo";"jasypoteĩ"; \
#          "jasypokõi";"jasypoapy";"jasyporundy";"jasypa";"jasypateĩ";"jasypakõi"
mon        "jasyte<U0129>";"jasyk<U00F5>i";"jasy<U02BC>apy";"jasyrundy";"jasypo";"jasypote<U0129>"; \
           "jasypok<U00F5>i";"jasypo<U02BC>apy";"jasyporundy";"jasypa";"jasypate<U0129>";"jasypak<U00F5>i"

#          "apo, 04/08/2011, 14:11:36 BRT"
d_t_fmt    "%a, %d/%m/%Y, %H:%M:%S %Z"

#          "04/08/2011"
d_fmt      "%d/%m/%Y"

#          "14:12:36"
t_fmt      "%H:%M:%S"

am_pm      "am";"pm"

#          "02:12:36 pm"
t_fmt_ampm "%I:%M:%S %p"

#           "apo, 04/08/2011, 14:11:36 BRT"
date_fmt    "%a, %d/%m/%Y, %H:%M:%S %Z"

# A semana tem 7 dias, como de costume, e começa na segunda-feira (ISO 8601)
week    7;19971130;4

# O primeiro dia da semana é a segunda-feira
first_weekday 2

# O primeiro dia de trabalho da semana é a segunda-feira
first_workday 2

END LC_TIME



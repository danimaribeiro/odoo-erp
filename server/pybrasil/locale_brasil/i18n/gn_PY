#
# Definição dos dígrafos e letras especiais
#
collating-symbol <an-guarani>  # Ãã
collating-symbol <en-guarani>  # Ẽẽ
collating-symbol <in-guarani>  # Ĩĩ
collating-symbol <on-guarani>  # Õõ
collating-symbol <un-guarani>  # Ũũ
collating-symbol <yn-guarani>  # Ỹỹ
collating-symbol <nn-guarani>  # Ññ
collating-symbol <puso-guarani>  # '

collating-symbol <gn-guarani>  # G̃g̃
collating-element <GN> from "<U0047><U0303>"
collating-element <gn> from "<U0067><U0303>"

collating-symbol <ch-guarani>
collating-element <CH> from "<U0043><U0048>"
collating-element <Ch> from "<U0043><U0068>"
collating-element <cH> from "<U0063><U0048>"
collating-element <ch> from "<U0063><U0068>"

collating-symbol <mb-guarani>
collating-element <MB> from "<U004D><U0042>"
collating-element <Mb> from "<U004D><U0062>"
collating-element <mB> from "<U006D><U0042>"
collating-element <mb> from "<U006D><U0062>"

collating-symbol <nd-guarani>
collating-element <ND> from "<U004E><U0044>"
collating-element <Nd> from "<U004E><U0064>"
collating-element <nD> from "<U006E><U0044>"
collating-element <nd> from "<U006E><U0064>"

collating-symbol <ng-guarani>
collating-element <NG> from "<U004E><U0047>"
collating-element <Ng> from "<U004E><U0067>"
collating-element <nG> from "<U006E><U0047>"
collating-element <ng> from "<U006E><U0067>"

collating-symbol <nt-guarani>
collating-element <NT> from "<U004E><U0054>"
collating-element <Nt> from "<U004E><U0074>"
collating-element <nT> from "<U006E><U0054>"
collating-element <nt> from "<U006E><U0074>"

collating-symbol <rr-guarani>
collating-element <RR> from "<U0052><U0052>"
collating-element <Rr> from "<U0052><U0072>"
collating-element <rR> from "<U0072><U0052>"
collating-element <rr> from "<U0072><U0072>"

#
# Difinas, ke la E-aj literoj estos ordigitaj
# post ĝia ne-ĉapelitaj versioj, kiel apartaj
# literoj
#
reorder-after <a>
<an-guarani>

reorder-after <b>
<ch-guarani>

reorder-after <e>
<en-guarani>

reorder-after <g>
<gn-guarani>

reorder-after <i>
<in-guarani>

reorder-after <m>
<mb-guarani>

reorder-after <n>
<nd-guarani>
<ng-guarani>
<nt-guarani>
<nn-guarani>

reorder-after <o>
<on-guarani>

reorder-after <r>
<rr-guarani>

reorder-after <u>
<un-guarani>

reorder-after <y>
<yn-guarani>
<puso-guarani>

reorder-after <an-guarani>

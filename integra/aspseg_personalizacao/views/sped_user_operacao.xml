<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_sped_users_fone" model="ir.ui.view">
            <field name="name">sped.users.fone</field>
            <field name="model">res.users</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="sped_base.view_sped_users_fone"/>
            <field name="arch" type="xml">             
                <field name="user_email" position="before">
                    <field name="operacao_id" colspan="4"/>                    
                </field>                                  
            </field>
        </record> 
        
        <record id="view_sped_users_fone_form" model="ir.ui.view">
            <field name="name">sped.users.fone.form</field>
            <field name="model">res.users</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="sped_base.view_sped_users_fone_form"/>
            <field name="arch" type="xml">             
                <field name="user_email" position="before">
                    <group colspan="4">                        
                        <field name="operacao_id" colspan="4"/>                                        
                    </group>
                </field>                                  
            </field>
        </record> 
    </data>
</openerp>

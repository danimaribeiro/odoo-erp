	<queryString>
		<![CDATA[select
sum(coalesce(
case
    when situacao = 'DF' THEN ch.valor
    else 0
end, 0)) as valor_devolvido,

sum(coalesce(
case
    when situacao != 'DF' THEN ch.valor
    else 0
end
,0)) as valor_cheques

from
  finan_cheque ch
  join res_partner_bank b on b.id = ch.res_partner_bank_id

where
  ch.situacao != 'DP'
  $P!{RES_PARTNER_BANK_ID_SUB}]]>
	</queryString>

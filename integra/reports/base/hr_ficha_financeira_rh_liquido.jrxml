	<parameter name="CONTRACT_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select

sum(coalesce(
case
    when pc.competencia like '%-01' then pc.valor
    else 0
end, 0) *
case when
pc.sinal = '+' then 1
else -1
end
) as valor_janeiro,


sum(coalesce(
case
    when pc.competencia like '%-02' then pc.valor
    else 0
end, 0) *
case when
pc.sinal = '+' then 1
else -1
end
) as valor_fevereiro,

sum(coalesce(
case
    when pc.competencia like '%-03' then pc.valor
    else 0
end, 0) *
case when
pc.sinal = '+' then 1
else -1
end
) as valor_marco,

sum(coalesce(
case
    when pc.competencia like '%-04' then pc.valor
    else 0
end, 0)  *
case when
pc.sinal = '+' then 1
else -1
end
) as valor_abril,

sum(coalesce(
case
    when pc.competencia like '%-05' then pc.valor
    else 0
end, 0)  *
case when
pc.sinal = '+' then 1
else -1
end
) as valor_maio,


sum(coalesce(
case
    when pc.competencia like '%-06' then pc.valor
    else 0
end, 0)
 *
case when
pc.sinal = '+' then 1
else -1
end
) as valor_junho,

sum(coalesce(
case
    when pc.competencia like '%-07' then pc.valor
    else 0
end, 0)
) as valor_julho,

sum(coalesce(
case
    when pc.competencia like '%-08' then pc.valor
    else 0
end, 0)  *
case when
pc.sinal = '+' then 1
else -1
end
) as valor_agosto,

sum(coalesce(
case
    when pc.competencia like '%-09' then pc.valor
    else 0
end, 0)  *
case when
pc.sinal = '+' then 1
else -1
end
) as valor_setembro,

sum(coalesce(
case
    when pc.competencia like '%-10' then pc.valor
    else 0
end, 0)  *
case when
pc.sinal = '+' then 1
else -1
end
) as valor_outubro,

sum(coalesce(
case
    when pc.competencia like '%-11' then pc.valor
    else 0
end, 0)
) as valor_novembro,

sum(coalesce(
case
    when pc.competencia like '%-12' then pc.valor
    else 0
end, 0)  *
case when
pc.sinal = '+' then 1
else -1
end
) as valor_dezembro,


sum(coalesce(
case
    when pc.competencia like '%-13' then pc.valor
    else 0
end, 0)  *
case when
pc.sinal = '+' then 1
else -1
end
) as valor_13,

sum(coalesce(
case
    when pc.sinal = '+' then pc.valor
    else 0
end, 0))
as valor_proventos,

sum(coalesce(
case
    when pc.sinal = '-' then pc.valor
    else 0
end, 0))
as valor_descontos

from hr_payslip_line_competencia pc
join hr_employee e on e.id = pc.employee_id

where
pc.competencia like '2014%'
and pc.contract_id = $P{CONTRACT_ID}
and pc.sinal in ('+','-')]]>
	</queryString>

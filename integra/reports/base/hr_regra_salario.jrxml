	<queryString>
		<![CDATA[select
rp.name as empresa,
sr.name as nome_rubrica,
sr.codigo,
sr.code,
sr.sequence,
sr.active,
sr.manual as lancamento_manual,
sr.manual_horas,
sr.afastamento,
sr.codigo_afastamento,
case
when sr.tipo_media = 'valor' then
'Valor'
when sr.tipo_media = 'quantidade' then
'Quantidade'
when sr.tipo_media = 'calculada' then
'Calcula proporcional'
when sr.tipo_media = 'afastamento' then
'Dias de afastamento'
end
as tipo_media,
sr.ignora_media_13,
src.name as categoria,
psa.name as estrutura_afastamento,
sra.name as holerite_anterior,
srd.name as saldo_devedor,
rr.descricao,
sr.condition_python as condicao_python,
sr.amount_python_compute,
case
when sr.finan_agrupamento = 'F' then
'Funcionário'
when sr.finan_agrupamento = 'E' then
'Empresa/unidade'
when sr.finan_agrupamento = 'C' then
'CNPJ da empresa/unidade'
when sr.finan_agrupamento = 'S' then
'Sindicato'
end as finan_agrupamento,
cfd.nome as finan_despesa,
cfdf.nome as finan_despesa_feria,
cfdr.nome as finan_despesa_rescisao,
cfdd.nome as finan_despesa_13,
cfc.nome as finan_custo,
cfcf.nome as finan_custo_feria,
cfcr.nome as finan_custo_rescisao,
cfcd.nome as finan_custo_13,
cov.name as convenio,
smod.nome as modelo_despesa,
smodf.nome as modelo_despesa_feria,
smodr.nome as modelo_despesa_rescisao,
smodd.nome as modelo_despesa_13,

smoc.nome as modelo_custo,
smocf.nome as modelo_custo_feria,
smocr.nome as modelo_custo_rescisao,
smocd.nome as modelo_custo_13

from hr_salary_rule sr
join hr_salary_rule_category src on src.id = sr.category_id
left join res_company c on c.id = sr.company_id
left join res_partner rp on rp.id = c.partner_id
left join hr_payroll_structure psa on psa.id = sr.estrutura_afastamento_id
left join hr_salary_rule sra on sra.id = sr.regra_holerite_anterior_id
left join hr_salary_rule srd on srd.id = sr.regra_saldo_devedor_id
left join hr_rubrica_rescisao rr on rr.id = sr.rubrica_rescisao_id
left join res_partner cov on cov.id = sr.partner_id

left join finan_conta cfd on cfd.id = sr.finan_conta_despesa_id
left join finan_conta cfdf on cfdf.id = sr.finan_conta_despesa_ferias_id
left join finan_conta cfdr on cfdr.id = sr.finan_conta_despesa_rescisao_id
left join finan_conta cfdd on cfdd.id = sr.finan_conta_despesa_13_id

left join finan_conta cfc on cfc.id = sr.finan_conta_custo_id
left join finan_conta cfcf on cfcf.id = sr.finan_conta_custo_ferias_id
left join finan_conta cfcr on cfcr.id = sr.finan_conta_custo_rescisao_id
left join finan_conta cfcd on cfcd.id = sr.finan_conta_custo_13_id

left join sped_modelo_partida_dobrada smod on smod.id = sr.modelo_folha_despesa_id
left join sped_modelo_partida_dobrada smodf on smodf.id = sr.modelo_folha_despesa_ferias_id
left join sped_modelo_partida_dobrada smodr on smodr.id = sr.modelo_folha_despesa_rescisao_id
left join sped_modelo_partida_dobrada smodd on smodd.id = sr.modelo_folha_despesa_13_id

left join sped_modelo_partida_dobrada smoc on smoc.id = sr.modelo_folha_custo_id
left join sped_modelo_partida_dobrada smocf on smocf.id = sr.modelo_folha_custo_ferias_id
left join sped_modelo_partida_dobrada smocr on smocr.id = sr.modelo_folha_custo_rescisao_id
left join sped_modelo_partida_dobrada smocd on smocd.id = sr.modelo_folha_custo_13_id

where
sr.id = $P{REGISTRO_ID}]]>
	</queryString>

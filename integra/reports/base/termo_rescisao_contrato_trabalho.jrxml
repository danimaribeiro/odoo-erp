	<parameter name="REGISTRO_IDS" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
--Empresa
rp.razao_social as razao_social,
u.cnpj_cpf,
cn.codigo as empresa_cnae,
rp.cidade as empresa_cidade,
rp.estado as empresa_estado,
rp.endereco as empresa_endereco,
rp.numero as empresa_numero,
rp.complemento as empresa_complemento,
rp.bairro as empresa_bairro,
rp.cep as empresa_cep,

--Empregado
c.name as matricula_empregado,
e.nome as nome_empregado,
e.endereco as endereco_empregado,
e.numero as numero_empregado,
e.bairro as bairro_empregado,
e.cidade as cidade_empregado,
e.estado as estado_empregado,
e.cep as cep_empregado,
e.fone as fone_empregado,
e.celular as celular_empregado,
e.cpf as cpf_empreago,
e.nis,
e.data_nascimento as data_nascimento_empregado,
e.nome_mae,
e.nome_pai,
e.carteira_trabalho_numero,
e.carteira_trabalho_serie,
e.carteira_trabalho_estado,
e.rg_numero,
e.rg_orgao_emissor,
e.rg_data_expedicao,
p.nome as nacionalidade,

e.roc_numero,
e.roc_orgao_emissor,
e.roc_data_expedicao,
e.roc_data_validade,
e.casa_propria,
e.casa_propria_fgts,
e.deficiente_motor,
e.deficiente_visual,
e.deficiente_auditivo,
e.deficiente_reabilitado,
e.deficiente_obs,

c.date_start as data_admissao,
j.name as cargo,
hrs.codigo_afastamento_cef as cd_afastamento,
hrs.name as causa_afastamento,
case
  when htc.codigo_caixa is null or htc.codigo_caixa = '01' then '01 - Empregado'
  when htc.codigo_caixa = '03' then '03 - Trabalhador não vinculado ao RGPS, mas com direito ao FGTS'
  when htc.codigo_caixa = '04' then '04 - Empregado sob contrato de trabalho por prazo determinado'
  when htc.codigo_caixa = '06' then '06 - Empregado doméstico'
  when htc.codigo_caixa = '07' then '07 - Menor aprendiz'
  else htc.codigo_caixa
end as categoria_trabalhador,

case
when c.tipo_contrato = '1'
    then '1 - Contrato de trabalho por prazo indeterminado'
    else '2 - Contrato de trabalho por prazo determinado'
end
as tipo_contrato,

h.data_afastamento as data_afastamento,
h.data_aviso_previo as data_aviso_previo,

case
  when coalesce((select hl.total
from hr_payslip_line hl
join hr_payslip hh on hh.id = hl.slip_id
where
hl.code = 'BRUTO'
and hh.tipo = 'N'
and hh.contract_id = h.contract_id
and hh.date_to <= h.date_from
order by
hh.date_to desc
limit 1
), 0) = 0 then
    case
        when c.unidade_salario = '1' then coalesce(c.wage, 0) * coalesce(c.horas_mensalista, 220)
        else coalesce(c.wage, 0)
    end
else
coalesce((select hl.total
from hr_payslip_line hl
join hr_payslip hh on hh.id = hl.slip_id
where
hl.code = 'BRUTO'
and hh.tipo = 'N'
and hh.contract_id = h.contract_id
and hh.date_to <= h.date_from
order by
hh.date_to desc
limit 1
), 0)
end as salario_anterior,

(select hl.total
from hr_payslip_line hl
where
hl.slip_id = h.id
and upper(hl.code) = 'PENSAO_ALIMENTICIA'
) as pensao_alimenticia,

(select hl.total
from hr_payslip_line hl
where
hl.slip_id = h.id
and hl.code = 'LIQ'
) as liquido_rescisao,

cbo.codigo as cbo_codigo,
cbo.ocupacao as cbo_descricao,


s.name as sindicato,
s.cnpj_cpf as sindicato_cnpj,
s.codigo_sindical as codigo_sindical,
h.id,
t.cnpj_cpf as tomador_cnpj_cpf

from hr_employee e
join hr_contract c on c.employee_id = e.id
join hr_job j on j.id = c.job_id
left join hr_cbo cbo on cbo.id = j.cbo_id
join res_company u on u.id = c.company_id
join res_partner rp on rp.id = u.partner_id
join sped_cnae cn on cn.id = rp.cnae_id
left join sped_municipio mn on mn.id = e.municipio_nascimento_id
left join sped_estado en on en.id = mn.estado_id
left join sped_pais p on p.id = e.pais_nacionalidade_id
left join res_partner s on s.id = c.sindicato_id
left join res_partner t on t.id = c.lotacao_id
join hr_payslip as h on h.contract_id = c.id and h.tipo = 'R'
left join hr_payroll_structure hrs on hrs.id = h.struct_id
left join hr_categoria_trabalhador as htc on htc.codigo = c.categoria_trabalhador

where
h.id in $P!{REGISTRO_IDS}

order by
rp.razao_social,
e.nome]]>

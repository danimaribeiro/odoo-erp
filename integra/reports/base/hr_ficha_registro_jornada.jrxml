	<parameter name="CONTRACT_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select hca.data_alteracao,
hj.descricao as seg_sex_jornada,
hjs.descricao as sabado_jornada,
hje.descricao as jornada_escala,

case
 when hca.jornada_escala = '1' then
 '12 × 36'
 when hca.jornada_escala = '2' then
'24 × 72'
else
'6 × 18'
 end as escala,

case
when hca.jornada_tipo = '1' then
'Padrão'
else
'Especial/escala'
end as jornada

from hr_contract_alteracao hca
left join hr_jornada hj on hj.id = hca.jornada_segunda_a_sexta_id
left join hr_jornada hjs on hjs.id = hca.jornada_sabado_id
left join hr_jornada hje on hje.id = hca.jornada_escala_id

where
hca.tipo_alteracao = 'J'
and hca.contract_id = $P{CONTRACT_ID}]]>
	</queryString>

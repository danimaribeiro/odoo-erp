	<parameter name="CONTRACT_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
     hca.data_alteracao as data_alteracao,
     hca.wage as salario,
     case
     when hca.unidade_salario = '1' then
     'Por hora'
     else
     'Por mês'
     end as unidade_salarial



from hr_contract_alteracao hca

where hca.tipo_alteracao = 'R'
and hca.contract_id = $P{CONTRACT_ID}]]>

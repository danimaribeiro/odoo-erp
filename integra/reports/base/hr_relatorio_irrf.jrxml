	<queryString>
		<![CDATA[select
  h.company_id,
  p.razao_social as empresa,
  coalesce(p.endereco, '') || ', ' || coalesce(p.numero, '') || ' - ' || coalesce(p.complemento, '') as empresa_endereco,
  coalesce(p.bairro, '') as empresa_bairro,
  coalesce(p.cidade, '') as empresa_cidade,
  coalesce(p.estado, '') as empresa_estado,
  coalesce(p.cep, '') as empresa_cep,
  p.cnpj_cpf as cnpj,
  e.id,
  e.nome,
  to_char(c.date_start, 'dd/mm/yyyy') as data_admissao,
  to_char(c.date_end, 'dd/mm/yyyy') as data_rescisao,
  c.name as matricula,
  case
  when c.categoria_trabalhador in ('701','702','703') then
  'AU'
  when c.categoria_trabalhador in ('721', '722') then
  'PL'
  when (h.tipo = 'N' and h.holerite_anterior_id is not null) or h.tipo = 'F' then
  'FE'
  when h.tipo = 'N' and exists(select pa.id from hr_payslip_afastamento pa where pa.payslip_id = h.id) then
  'AF'
  when h.tipo = 'N' then
  'NR'
  when h.tipo = 'R' then
  'RE'
  when h.tipo = 'D' then
  'DT'
  end as holerite_tipo,

  case
  when c.categoria_trabalhador in ('701','702','703') then
  '0588'
  else '0561'
  end as codigo_recolhimento,

  case
  when c.categoria_trabalhador in ('701','702','703') then
  'Autônomo'
  when c.categoria_trabalhador in ('721', '722') then
  'Pro-labore'
  when (h.tipo = 'N' and h.holerite_anterior_id is not null) or h.tipo = 'F' then
  'Férias'
  when h.tipo = 'N' and exists(select pa.id from hr_payslip_afastamento pa where pa.payslip_id = h.id) then
  'Afastado'
  when h.tipo = 'N' then
  'Normal'
  when h.tipo = 'R' then
  'Rescisão'
  when h.tipo = 'D' then
  'Décimo Terceiro'
  end as holerite_tipo_descricao,

  h.id as holerite_id,

  coalesce((SELECT sum(hpl.amount)
  FROM hr_payslip_line as hpl
  where hpl.code in ('BASE_INSS', 'BASE_INSS_13', 'BASE_INSS_13_AP') and hpl.slip_id = h.id), 0) as base_inss,

  coalesce((SELECT sum(hpl.total * case when hpl.code = 'INSS_FERIAS_GOZADAS' then -1 else 1 end)
  FROM hr_payslip_line as hpl
  where hpl.code in ('INSS', 'INSS_13', 'INSS_13_AP', 'INSS_anterior', 'INSS_FERIAS_GOZADAS')
  and hpl.slip_id = h.id), 0) as inss,

  coalesce((SELECT sum(hpl.total)
  FROM hr_payslip_line as hpl
  where hpl.code = 'IRPF' and hpl.slip_id = h.id), 0) as irpf,

  coalesce((SELECT sum(hpl.total)
  FROM hr_payslip_line as hpl
  where hpl.code = 'DEDUCAO_DEPENDENTES' and hpl.slip_id = h.id), 0) as deducao_dependentes,

  (select tdd.deducao_dependente from hr_tabela_ir_dependente tdd where tdd.ano = h.ano and tdd.mes <= h.mes order by tdd.ano desc, tdd.mes desc limit 1) as deducao_dependente,

  (select tpd.parcela_deduzir from hr_tabela_ir tpd where tpd.ano = h.ano and tpd.mes <= h.mes and tpd.piso <= h.base_irpf order by tpd.ano desc, tpd.mes desc, tpd.piso desc limit 1) as parcela_deduzir,

 coalesce((select coalesce(hl.rate, 0) from hr_payslip_line hl where hl.slip_id = h.id and coalesce(hl.holerite_anterior_line_id, null) is null and hl.code = 'IRPF'), 0) as aliquota_irpf,
  h.data_pagamento_irpf,
  h.data_vencimento_irpf,
  to_char(h.data_vencimento_irpf, 'MM/YYYY') as competencia


from
  hr_payslip h
  join hr_employee e on e.id = h.employee_id
  join hr_contract c on c.id = h.contract_id
  join res_company cp on cp.id = c.company_id
  join res_partner p on p.id = cp.partner_id

where
  h.tipo in $P!{TIPOS} and (h.simulacao = False or h.simulacao is null)
  and (($P{COMPLEMENTAR} = True and h.complementar = $P{COMPLEMENTAR}) or ($P{COMPLEMENTAR} = False and (h.complementar is null or h.complementar = False)))
  and ((
      (h.complementar is null or h.complementar = False)
      and h.data_pagamento_irpf between '$P!{DATA_INICIAL}'  and '$P!{DATA_FINAL}'
     )
    or (
      h.complementar = True
      and h.data_pagamento_irpf between '$P!{DATA_INICIAL}'  and '$P!{DATA_FINAL}'
    ))

  and (
     cp.id = $P{COMPANY_ID}
     or cp.parent_id = $P{COMPANY_ID}
  )
  and h.valor_irpf > 0

order by
  p.cnpj_cpf,
  holerite_tipo,
  e.nome]]>

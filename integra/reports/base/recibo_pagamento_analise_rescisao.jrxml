	<queryString>
		<![CDATA[select
case
  when h.tipo = 'N' then 'RECIBO DE PAGAMENTO - SALÁRIO'
  when h.tipo = 'D' then 'RECIBO DE PAGAMENTO - 13º SALÁRIO'
  when h.tipo = 'R' then 'ANÁLISE DE RESCISÃO'
end as tipo,
p.razao_social,
p.cnpj_cpf,
p.endereco as empresa_endereco,
p.numero as empresa_numero,
p.complemento as empresa_complemento,
p.bairro as empresa_bairro,
p.cidade as empresa_cidade,
p.estado as empresa_estado,
p.cep as empresa_cep,
e.nome,
c.name as matricula,
e.carteira_trabalho_numero,
e.carteira_trabalho_serie,
c.date_start as data_admissao,
cast(case
when exists (select em.data_nascimento
from
hr_employee em
where
to_char(em.data_nascimento,'MM') between to_char(h.date_from + interval '1 month','MM') and to_char(h.date_to + interval '1 month','MM')
and em.id = e.id) then
'*** FELIZ ANIVERSÁRIO ***'
else
''
end   as varchar) as aniversario,
(cbo.codigo || ' - ' || j.name) as funcao,
coalesce((select count(*) from hr_employee_dependente d where d.employee_id =
e.id), 0) as dependentes,
case
    when (select ca.wage from hr_contract_alteracao ca where ca.tipo_alteracao = 'R' and ca.contract_id = c.id and ca.data_alteracao <= h.date_to order by ca.data_alteracao desc limit 1) is null then c.wage
    else (select ca.wage from hr_contract_alteracao ca where ca.tipo_alteracao = 'R' and ca.contract_id = c.id and ca.data_alteracao <= h.date_to order by ca.data_alteracao desc limit 1)
end as salario_base,
h.data_inicio_periodo_aquisitivo,
h.data_fim_periodo_aquisitivo,
h.date_from as data_inicio_gozo,
h.date_to as data_fim_gozo,
h.data_aviso_ferias,
b.bic as banco_codigo,
b.name as banco_nome,
e.banco_agencia,
e.banco_conta,
h.id,
cast(u.photo as varchar) as photo,
h.date_from -2 as data_pagamento,
(SELECT
cast(sum(
case
when sr.sinal = '+' then hl.total
else hl.total * -1
end) as numeric(18,2)) as liquido

FROM hr_payslip_line as hl inner join hr_salary_rule as sr on
hl.salary_rule_id = sr.id
where sr.sinal in ('+','-') and hl.total > 0 and hl.slip_id = h.id) as liquido,
((SELECT
cast(sum(
case
when sr.sinal = '+' then hl.total
else hl.total * -1
end) as numeric(18,2)) as liquido

FROM hr_payslip_line as hl inner join hr_salary_rule as sr on
hl.salary_rule_id = sr.id
where sr.sinal in ('+','-') and hl.total > 0 and hl.slip_id = h.id)) as liquido_extenso,

coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'BASE_INSS' and hl.slip_id = h.id), 0) as base_inss,
coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'INSS' and hl.slip_id = h.id), 0) as inss,

coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'BASE_INSS_13' and hl.slip_id = h.id), 0) as base_inss_13,
coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'INSS_13' and hl.slip_id = h.id), 0) as inss_13,

coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'BASE_FGTS' and hl.slip_id = h.id), 0) as base_fgts,
coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'FGTS' and hl.slip_id = h.id), 0) as fgts,

coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'BASE_IRPF' and hl.slip_id = h.id), 0) as base_irpf,
coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'IRPF' and hl.slip_id = h.id), 0) as irpf,

coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'BASE_IRPF_13' and hl.slip_id = h.id), 0) as base_irpf_13,
coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'IRPF_13' and hl.slip_id = h.id), 0) as irpf_13,

d.name as departamento,

mes_extenso(cast(h.date_from as date))  as competencia,
case
  when t.fantasia is not null then t.fantasia
  else t.razao_social
end as tomador,
case
    when
        coalesce((SELECT sum(hl.quantity) FROM hr_payslip_line as hl where hl.code = 'DIAS_FALTA' and hl.slip_id = h.id), 0) > 0
        then
	    'Dias de falta (' || trim(to_char((SELECT sum(hl.quantity) FROM hr_payslip_line as hl where hl.code = 'DIAS_FALTA' and hl.slip_id = h.id), '999')) || ') R$ ' || formata_valor(coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'DIAS_FALTA' and hl.slip_id = h.id), 0))
    else ''
end as dias_falta,
case
    when
        coalesce((SELECT sum(hl.quantity) FROM hr_payslip_line as hl where hl.code = 'DIAS_DSR_FALTA' and hl.slip_id = h.id), 0) > 0
        then
	    'DSR de falta (' || trim(to_char((SELECT sum(hl.quantity) FROM hr_payslip_line as hl where hl.code = 'DIAS_DSR_FALTA' and hl.slip_id = h.id), '999')) || ') R$ ' || formata_valor(coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'DIAS_DSR_FALTA' and hl.slip_id = h.id), 0))
    else ''
end as dias_dsr_falta,
case
    when
        coalesce((SELECT sum(hl.quantity) FROM hr_payslip_line as hl where hl.code = 'HORAS_FALTA_SUSPENSAO' and hl.slip_id = h.id), 0) > 0
        then
	    'Horas de suspensão (' || formata_valor((SELECT sum(hl.quantity) FROM hr_payslip_line as hl where hl.code = 'HORAS_FALTA_SUSPENSAO' and hl.slip_id = h.id)) || ') R$ ' || formata_valor(coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'HORAS_FALTA_SUSPENSAO' and hl.slip_id = h.id), 0))
    else ''
end as horas_falta_suspensao,
case
    when
        coalesce((SELECT sum(hl.quantity) FROM hr_payslip_line as hl where hl.code = 'DIAS_SUSPENSAO' and hl.slip_id = h.id), 0) > 0
        then
	    'Dias de suspensão (' || formata_valor((SELECT sum(hl.quantity) FROM hr_payslip_line as hl where hl.code = 'DIAS_SUSPENSAO' and hl.slip_id = h.id)) || ') R$ ' || formata_valor(coalesce((SELECT sum(hl.total) FROM hr_payslip_line as hl where hl.code = 'DIAS_SUSPENSAO' and hl.slip_id = h.id), 0))
    else ''
end as dias_suspensao,
h.data_afastamento as data_afastamento,
h.date_to,
es.name as estrutura


--from hr_payslip_line lh
--join hr_salary_rule r on r.id = lh.salary_rule_id
--join hr_payslip h on h.id = lh.slip_id
from hr_payslip h
join hr_employee e on e.id = h.employee_id
join hr_contract c on c.id = h.contract_id
join res_company u on u.id = h.company_id
join res_partner p on p.id = u.partner_id
join hr_job j on j.id = c.job_id
join hr_cbo cbo on cbo.id = j.cbo_id
left join res_bank b on b.id = e.bank_id
left join hr_department d on d.id = c.department_id
left join res_partner t on t.id = c.lotacao_id
join hr_payroll_structure es on es.id = h.struct_id
where
h.tipo in ('N', 'D', 'R')
and h.id in $P!{REGISTRO_IDS}

order by
  p.razao_social,
  e.nome]]>
	</queryString>

	<parameter name="CONTRACT_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
 hca.data_alteracao,
 rp.cnpj_cpf,
 rp.name as empresa,
 hd.name as departamento


from hr_contract_alteracao hca
left join hr_department hd on hd.id = hca.department_id
left join res_partner rp on rp.id = hca.lotacao_id

where
hca.tipo_alteracao = 'L'
and hca.contract_id = $P{CONTRACT_ID}

order by
hca.data_alteracao desc]]>
	</queryString>

	</field>
	<field name="valor" class="java.math.BigDecimal">
		<fieldDescription><![CDATA[Valor total]]></fieldDescription>
	</field>
	<field name="provento" class="java.math.BigDecimal"/>
	<field name="desconto" class="java.math.BigDecimal">

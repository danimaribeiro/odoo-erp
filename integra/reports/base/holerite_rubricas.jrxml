	<variable name="liquido" class="java.lang.Double">
		<variableExpression><![CDATA[$V{provento_1}.subtract($V{desconto_1})]]></variableExpression>
		<initialValueExpression><![CDATA[]]></initialValueExpression>
	</variable>

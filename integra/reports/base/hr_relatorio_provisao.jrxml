	<queryString>
		<![CDATA[select
  h.company_id,
  p.razao_social as empresa,
  coalesce(p.endereco, '') || ', ' || coalesce(p.numero, '') || ' - ' || coalesce(p.complemento, '') as empresa_endereco,
  coalesce(p.bairro, '') as empresa_bairro,
  coalesce(p.cidade, '') as empresa_cidade,
  coalesce(p.estado, '') as empresa_estado,
  coalesce(p.cep, '') as empresa_cep,
  to_char((select dia_util_pagamento from dia_util_pagamento(to_char(cast(to_char(cast($P{DATA_INICIAL} as date), 'yyyy-mm-20') as date) +  interval '1 month', 'yyyy-mm-dd'), upper(coalesce(p.estado, '')), upper(coalesce(p.cidade, '')), True)), 'dd/mm/yyyy') as data_pagamento,
  p.cnpj_cpf as cnpj,
  e.id,
  e.nome,
  to_char(c.date_start, 'dd/mm/yyyy') as data_admissao,
  to_char(c.date_end, 'dd/mm/yyyy') as data_rescisao,
  c.name as matricula,
--  sum(case when rc.sinal = '0' then hl.total else 0 end) as base_calculo,
--  sum(case when rc.sinal = '+' then hl.total else 0 end) as proventos,
--  sum(case when rc.sinal = '-' then hl.total else 0 end) as deducoes,
--  sum(case when rc.sinal = '+' then hl.total when rc.sinal = '-' then hl.total * -1 else 0 end) as liquido,
  case
  when c.categoria_trabalhador in ('701','702','703') then
  'AU'
  when c.categoria_trabalhador in ('721', '722') then
  'PL'
  when (h.tipo = 'N' and h.holerite_anterior_id is not null) or (h.tipo = 'F') then
  'FE'
  when h.tipo = 'N' and exists(select pa.id from hr_payslip_afastamento pa where pa.payslip_id = h.id) then
  'AF'
  when h.tipo = 'N' then
  'NR'
  when h.tipo = 'R' then
  'RE'
  when h.tipo = 'D' then
  'DT'
  end as holerite_tipo,

  case
  when c.categoria_trabalhador in ('701','702','703') then
  'Autônomo'
  when c.categoria_trabalhador in ('721', '722') then
  'Pro-labore'
  when (h.tipo = 'N' and h.holerite_anterior_id is not null) or (h.tipo = 'F') then
  'Férias'
  when h.tipo = 'N' and exists(select pa.id from hr_payslip_afastamento pa where pa.payslip_id = h.id) then
  'Afastado'
  when h.tipo = 'N' then
  'Normal'
  when h.tipo = 'R' then
  'Rescisão'
  when h.tipo = 'D' then
  'Décimo Terceiro'
  end as holerite_tipo_descricao,

  h.id as holerite_id,

  coalesce((
    SELECT sum(hpl.amount)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    where
        hpl.code in ('BASE_INSS', 'BASE_INSS_13', 'BASE_INSS_13_AP')
        and hpl.slip_id = h.id
  ), 0) as base_inss,

  coalesce((
    SELECT sum(hpl.amount)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    where
        hpl.code in ('BASE_INSS', 'BASE_INSS_13', 'BASE_INSS_13_AP')
        and hp.id = prov_anterior.id
        and (h.tipo = 'F' or h.mes != '01')
  ), 0) as base_inss_anterior,

  coalesce((
    SELECT sum(hpl.total * case when hpl.code = 'INSS_FERIAS_GOZADAS' then -1 else 1 end)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    where
        hpl.code in ('INSS', 'INSS_13', 'INSS_13_AP', 'INSS_DIF_13')
        and hpl.slip_id = h.id
  ), 0) as inss,

  coalesce((
    SELECT sum(hpl.total * case when hpl.code = 'INSS_FERIAS_GOZADAS' then -1 else 1 end)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    where
        hpl.code in ('INSS', 'INSS_13', 'INSS_13_AP', 'INSS_DIF_13')
        and hp.id = prov_anterior.id
        and (h.tipo = 'F' or h.mes != '01')
  ), 0) as inss_anterior,

  coalesce((
    SELECT sum(hpl.total)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    where
        hpl.code in ('INSS_EMPRESA')
        and hpl.slip_id = h.id
  ), 0) as inss_empresa,

  coalesce((
    SELECT sum(hpl.total)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    where
        hpl.code in ('INSS_EMPRESA')
        and hp.id = prov_anterior.id
        and (h.tipo = 'F' or h.mes != '01')
  ), 0) as inss_empresa_anterior,

  coalesce((
    SELECT sum(hpl.total)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    where
        hpl.code in ('INSS_RAT')
        and hpl.slip_id = h.id
  ), 0) as inss_rat,

  coalesce((
    SELECT sum(hpl.total)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    where
        hpl.code in ('INSS_RAT')
        and hp.id = prov_anterior.id
        and (h.tipo = 'F' or h.mes != '01')
  ), 0) as inss_rat_anterior,

  coalesce((
    SELECT sum(hpl.total)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    where
        hpl.code in ('INSS_OUTRAS_ENTIDADES')
        and hpl.slip_id = h.id
  ), 0) as inss_outros,

  coalesce((
    SELECT sum(hpl.total)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    where
        hpl.code in ('INSS_OUTRAS_ENTIDADES')
        and hp.id = prov_anterior.id
        and (h.tipo = 'F' or h.mes != '01')
  ), 0) as inss_outros_anterior,

  coalesce((
    SELECT sum(hpl.amount)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    join hr_payroll_structure es on es.id = hp.struct_id
    where
        hpl.code in ('FGTS')
        and hpl.slip_id = h.id
        and (hp.tipo != 'R' or es.codigo_afastamento not in ('H', 'I1', 'I2', 'I3', 'K'))
  ), 0) as base_fgts,

  coalesce((
    SELECT sum(hpl.amount)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    join hr_payroll_structure es on es.id = hp.struct_id
    where
        hpl.code in ('FGTS')
        and (hp.tipo != 'R' or es.codigo_afastamento not in ('H', 'I1', 'I2', 'I3', 'K'))
        and hp.id = prov_anterior.id
        and (h.tipo = 'F' or h.mes != '01')
  ), 0) as base_fgts_anterior,

  coalesce((
    SELECT sum(hpl.total)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    join hr_payroll_structure es on es.id = hp.struct_id
    where
        hpl.code in ('FGTS')
        and hpl.slip_id = h.id
        and (hp.tipo != 'R' or es.codigo_afastamento not in ('H', 'I1', 'I2', 'I3', 'K'))
  ), 0) as fgts,

  coalesce((
    SELECT sum(hpl.total)
    FROM hr_payslip_line as hpl
    join hr_payslip hp on hp.id = hpl.slip_id
    join hr_payroll_structure es on es.id = hp.struct_id
    where
        hpl.code in ('FGTS')
        and (hp.tipo != 'R' or es.codigo_afastamento not in ('H', 'I1', 'I2', 'I3', 'K'))
        and hp.id = prov_anterior.id
        and (h.tipo = 'F' or h.mes != '01')
  ), 0) as fgts_anterior,
  case
     when h.tipo = 'F' then prov_anterior.id
     when h.tipo = 'D' and h.mes != '01' then prov_anterior.id
     else null
  end as provisao_anterior_id,
  case
      when h.tipo = 'F' then to_char(h.data_inicio_periodo_aquisitivo, 'dd/mm/yyyy')
      when h.tipo = 'D' then to_char(h.data_inicio_periodo_aquisitivo, 'dd/mm/yyyy')
      else null
  end as data_inicio_periodo_aquisitivo,
  case
      when h.tipo = 'F' then to_char(h.data_fim_periodo_aquisitivo, 'dd/mm/yyyy')
      when h.tipo = 'D' then to_char(h.date_to, 'dd/mm/yyyy')
      else null
  end as data_fim_periodo_aquisitivo,
  formata_valor(cast(coalesce(h.dias_ferias, 0) as numeric)) as dias_ferias,
  case
      when h.tipo = 'F' then cast(coalesce(h.dias_ferias, 0) / 2.5 as integer)
      when h.tipo = 'D' then cast(h.meses_decimo_terceiro as integer)
      else 0
  end as avos


from
  hr_payslip h
  join hr_employee e on e.id = h.employee_id
  join hr_contract c on c.id = h.contract_id
  join res_company cp on cp.id = c.company_id
  join res_partner p on p.id = cp.partner_id
  left join hr_payslip prov_anterior on h.provisao = True and prov_anterior.contract_id = h.contract_id and prov_anterior.data_inicio_periodo_aquisitivo = h.data_inicio_periodo_aquisitivo and prov_anterior.tipo = h.tipo and to_char(prov_anterior.date_to, 'yyyy-mm') = to_char(h.date_to - interval '1 month', 'yyyy-mm')

where
  h.tipo in $P!{TIPOS}
  and h.provisao = True
  and (
       (h.tipo in ('D', 'F') and h.date_to between cast($P{DATA_INICIAL} as date) and cast($P{DATA_FINAL} as date))
       or (h.tipo = 'R' and h.data_afastamento between cast($P{DATA_INICIAL} as date) and cast($P{DATA_FINAL} as date))
      )

  and (
     cp.id = $P{COMPANY_ID}
     or cp.parent_id = $P{COMPANY_ID}
  )

order by
  p.cnpj_cpf,
  holerite_tipo,
  e.nome]]>
